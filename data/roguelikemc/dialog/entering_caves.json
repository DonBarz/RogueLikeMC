{
  "type": "minecraft:confirmation",
  "title": {
    "text": "Entering the Mines...",
    "color": "dark_gray",
    "bold": true
  },
  "external_title": "roguelikemc:entering_caves",
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": {
        "text": "Enter these ancient abandoned mines  and retake them from the undead."
      }
    },
    {
      "type": "minecraft:item",
      "item": {
        "id": "minecraft:zombie_spawn_egg"
      },
      "show_tooltip": false
    },
    {
      "type": "minecraft:item",
      "item": {
        "id": "minecraft:spider_spawn_egg"
      }
    },
    {
      "type": "minecraft:item",
      "item": {
        "id": "minecraft:skeleton_spawn_egg"
      }
    },
    {
      "type": "minecraft:item",
      "item": {
        "id": "minecraft:silverfish_spawn_egg"
      }
    }
  ],
  "inputs": [],
  "can_close_with_escape": true,
  "pause": true,
  "yes": {
    "label": {
      "text": "Ready!",
      "color": "green"
    },
    "action": {
      "type": "minecraft:run_command",
      "command": "function roguelikemc:dungeon/place_dungeon {type:\"level_1\"}"
    }
  },
  "no": {
    "label": {
      "text": "Cancel",
      "color": "red"
    },
    "action": {
      "type": "minecraft:show_dialog",
      "dialog": "roguelikemc:leave_base"
    }
  }
}